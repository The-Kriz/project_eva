<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:include filename="inertial_macros.xacro"/>

    <link name="right_sholder">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 3.14"/>
            <geometry>
                <mesh filename="file://$(find project_eva)/meshes/EVA/Arm/sholder.STL"/>
            </geometry>
            <material name="Top">
                <color rgba="${48/255} ${104/255} ${68/255} 1.0"/>
            </material>
        </visual>

        <collision>
            <origin xyz="0.0 -0.035 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.070 0.080 0.050 "/> 
            </geometry>
        </collision>

        <xacro:inertial_box mass="0.3" x="0.070" y="0.080" z="0.050">
            <origin xyz="0.0 -0.035 0.0" rpy="0.0 0.0 0.0"/>
        </xacro:inertial_box>

    </link>

    <joint name="right_arm_joint" type="revolute">
        <axis xyz="0 1 0"/>
        <limit effort="0.1" lower="-2.0" upper="0.51" velocity="0.2"/>
        <origin xyz="0 -0.118 0.457" rpy="3.14 3.14 3.14"/>
        <parent link="body" />
        <child link="right_sholder" />
    </joint>


