            </joint>

<!--//////////////////////////////////// head ///////////////////////////////////////////////////////////-->

            <joint name="neck_joint">
                <command_interface name="position">
                    <param name="min">-1</param>
                    <param name="max">1</param>
                </command_interface>
                <state_interface name="position"/>
            </joint>

            <joint name="head_joint">
                <command_interface name="position">
                    <param name="min">-.51</param>
                    <param name="max">0.38</param>
                </command_interface>
                <state_interface name="position"/>
            </joint>

