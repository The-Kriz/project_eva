<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:macro name="omni_wheel_roller" params="parent child xyz rpy ">
        <link name="${parent}_${child}_link">
            <visual>
                <origin xyz="0 0 0" rpy="1.57 0 0" />
                <geometry>
                    <mesh filename="file://$(find project_eva)/meshes/wheel_roller.STL"/>
                </geometry>
                <material name="Wheel_rubber">
                    <color rgba="0 0 0 1" />
                </material>
            </visual>

            <collision>
                <origin xyz="0 0 0" rpy="1.57 0 0" />
                <geometry>
                    <mesh filename="file://$(find project_eva)/meshes/wheel_roller.STL"/>
                </geometry>
            </collision>

            <inertial>
                <origin xyz="5.09395238803556E-16 6.79977710486667E-16 -0.0104983350650447" rpy="1.57 0 0" />
                <mass value="0.65" />
                <inertia ixx="0.000781592793267822" ixy="1.05247463532391E-18" ixz="-6.56787240457889E-19"
                         iyy="0.000781592793267821" iyz="-1.19446449980815E-19"
                         izz="0.0014442649663699" />
            </inertial>
        </link>

        <joint name="${parent}_${child}_joint" type="continuous">
            <parent link="${parent}" />
            <child link="${parent}_${child}_link" />
            <origin xyz="${xyz}" rpy="${rpy}" />
            <axis xyz="0.0 0.0 1.0" />
        </joint>
    </xacro:macro>

