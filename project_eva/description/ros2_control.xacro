                    <param name="min">-1.5</param>
                    <param name="max">1.10</param>
                </command_interface>
                <state_interface name="position"/>
            </joint>

            <joint name="right_finger_4_link_1_joint">
                <command_interface name="position">
                    <param name="min">0.0</param>
