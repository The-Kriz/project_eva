                <param name="multiplier">1</param>
                <command_interface name="position">
                <param name="min">-1.57</param>
                <param name="max">0.0</param>
                </command_interface>
            </joint>

            <joint name="left_finger_1_link_2_joint">
                <param name="mimic">left_finger_1_link_1_joint</param>
                <param name="multiplier">1</param>
